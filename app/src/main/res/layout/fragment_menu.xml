<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.Home.fragments.MenuFragment">

    <SearchView
        android:id="@+id/search_view"
        android:layout_width="335dp"
        android:layout_height="35dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/bg_filled_oval_orange"
        android:backgroundTint="@color/gray"
        android:queryHint="Search Food"
        app:layout_constraintBottom_toTopOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="100dp"
        android:layout_height="485dp"
        android:background="@drawable/shape_menu_left_orange"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout1"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_chevron1"
            style="@style/Widget.AppCompat.Button.IconOnly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="320dp"
            android:background="@drawable/shape_round_button"
            android:backgroundTint="@color/white"
            android:drawableStart="@drawable/ic_baseline_chevron_right"
            android:elevation="5dp"
            android:padding="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="56dp"
            android:layout_marginTop="20dp"
            android:elevation="4dp"
            android:text="Food"
            android:textColor="@color/black"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/view11"
            app:layout_constraintTop_toTopOf="@+id/view11" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="56dp"
            android:elevation="4dp"
            android:text="120 Items"
            android:textSize="11sp"
            app:layout_constraintStart_toStartOf="@+id/view11"
            app:layout_constraintTop_toBottomOf="@+id/textView1" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/circleImageView3"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="20dp"
            android:elevation="4dp"
            android:src="@drawable/photo_pizza"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view11" />

        <View
            android:id="@+id/view11"
            android:layout_width="280dp"
            android:layout_height="87dp"
            android:layout_marginLeft="60dp"
            android:background="@drawable/ripple_menu_item"
            android:clickable="true"
            android:elevation="1dp"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout1">

        <androidx.appcompat.widget.AppCompatButton
            style="@style/Widget.AppCompat.Button.IconOnly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="320dp"
            android:background="@drawable/shape_round_button"
            android:backgroundTint="@color/white"
            android:drawableStart="@drawable/ic_baseline_chevron_right"
            android:elevation="3dp"
            android:padding="6dp"
            android:translationZ="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="56dp"
            android:layout_marginTop="20dp"
            android:elevation="4dp"
            android:text="Beverages"
            android:textColor="@color/black"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/view21"
            app:layout_constraintTop_toTopOf="@+id/view21" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="56dp"
            android:elevation="4dp"
            android:text="220 Items"
            android:textSize="11sp"
            app:layout_constraintStart_toStartOf="@+id/view21"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="9dp"
            android:elevation="4dp"
            android:src="@drawable/photo_beverages"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view21" />

        <View
            android:id="@+id/view21"
            android:layout_width="280dp"
            android:layout_height="87dp"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="24dp"
            android:clickable="true"
            android:background="@drawable/ripple_menu_item"
            android:elevation="1dp"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <androidx.appcompat.widget.AppCompatButton
            style="@style/Widget.AppCompat.Button.IconOnly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="320dp"
            android:background="@drawable/shape_round_button"
            android:backgroundTint="@color/white"
            android:drawableStart="@drawable/ic_baseline_chevron_right"
            android:elevation="3dp"
            android:padding="6dp"
            android:translationZ="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="56dp"
            android:layout_marginTop="20dp"
            android:elevation="4dp"
            android:text="Desserts"
            android:textColor="@color/black"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/view31"
            app:layout_constraintTop_toTopOf="@+id/view31" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="56dp"
            android:elevation="4dp"
            android:text="155 Items"
            android:textSize="11sp"
            app:layout_constraintStart_toStartOf="@+id/view31"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="9dp"
            android:elevation="4dp"
            android:src="@drawable/photo_deserts"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view31" />

        <View
            android:id="@+id/view31"
            android:layout_width="280dp"
            android:layout_height="87dp"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="24dp"
            android:background="@drawable/ripple_menu_item"
            android:elevation="1dp"
            android:clickable="true"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/problem"
            style="@style/Widget.AppCompat.Button.IconOnly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="320dp"
            android:background="@drawable/shape_round_button"
            android:backgroundTint="@color/white"
            android:drawableStart="@drawable/ic_baseline_chevron_right"
            android:elevation="3dp"
            android:padding="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="56dp"
            android:layout_marginTop="20dp"
            android:elevation="4dp"
            android:text="Promotions"
            android:textColor="@color/black"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/view41"
            app:layout_constraintTop_toTopOf="@+id/view41" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="56dp"
            android:elevation="4dp"
            android:text="25 Items"
            android:textSize="11sp"
            app:layout_constraintStart_toStartOf="@+id/view41"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="9dp"
            android:elevation="4dp"
            android:src="@drawable/photo_promotions"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view41" />

        <View
            android:id="@+id/view41"
            android:layout_width="280dp"
            android:layout_height="87dp"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="24dp"
            android:background="@drawable/ripple_menu_item"
            android:elevation="1dp"
            android:clickable="true"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>